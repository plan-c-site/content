{% narrowBanner
   icon="circleAlert"
   color="red"
   iconAlign="items-center"
   fullWidth=true
   _language="" %}
{% textSize textSize="small" inline=false _language="" %}**Plan C does not sell pills or provide medical advice.** This guide lists verified results of our abortion pill research.{% /textSize %}
{% /narrowBanner %}

{% heroSection
   image={discriminant: "state", value: {altText: "Abortion pill %0%"}}
   imageSide="right"
   color="beige"
   imageBackgroundColor="beige"
   ignoreImageBorder=true
   imageCover="bg-contain"
   imageVertical="bg-center"
   imageHorizontal="center"
   mobileImageVertical="desktop"
   mobileImageHorizontal="left"
   largeLeftColumn=true
   reverseOnMobile=false
   _language="" %}
{% formWrapper
   formType={discriminant: "link", value: {pattern: "/abortion-pill/[:location:]"}}
   afterSubmit="nothing"
   _language="" %}
{% formSection type="waiting" _language="" %}
# Abortion pill providers in {% inlineForm slug="location" required=false autoSubmit=true options={discriminant: true, value: {valueType: "slug"}} _language="" %}{% dbText source="Name" _language="" %}State{% /dbText %}{% /inlineForm %}
{% /formSection %}
{% /formWrapper %}

{% dbMarkdoc source="Summary" _language="" /%}

{% tableOfContents _language="" /%}

{% conditionalSection
   condition={discriminant: "stateFieldTruthy", value: "Link to studies (in languages)"}
   _language="" %}
{% narrowBanner
   icon="alertCircle"
   color="beige"
   iconAlign="items-start"
   fullWidth=false
   _language="" %}
{% textSize textSize="small" inline=false _language="" %}If you live in this state and are seeking an abortion, you may qualify to participate in a paid research study about abortion access.&nbsp;{% inlineLink action={discriminant: "internal", value: {page: "research-and-learning", anchor: "participate-in-research"}} _language="" %}Click here to learn more{% /inlineLink %}.{% /textSize %}
{% /narrowBanner %}
{% /conditionalSection %}
{% /heroSection %}

{% tocAnchor
