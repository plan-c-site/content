.querySelectorAll(`tr > *:nth-child(${i})`).forEach(cell=>{\n      cell.setAttribute('data-mobile-hidden','');\n    });\n  });\n\n  // Hide on small screens; show on larger\n  function applyMobileHide(){\n    const hide = window.innerWidth < 768;\n    box.querySelectorAll('[data-mobile-hidden]').forEach(cell=>{\n      cell.style.display = hide ? 'none' : '';   // 'Show all columns' button will override this\n    });\n  }\n  window.addEventListener('resize', applyMobileHide);\n  applyMobileHide();\n})();\n</script>\n\n<script>\n(function(){\n  const tbl = document.querySelector('#provider-table') || document.querySelector('table');\n  if(!tbl) return;\n  tbl.style.borderCollapse = 'collapse';\n  tbl.querySelectorAll('thead th').forEach(th=>{\n    th.style.borderBottom = '2px solid #e5e7eb';\n  });\n  const rows = tbl.querySelectorAll('tbody tr');\n  rows.forEach((tr,i)=>{\n    tr.querySelectorAll('td').forEach(td=>{\n      td.style.borderBottom = (i === rows.length - 1) ? '0' : '1px solid #eee';\n    });\n  });\n})();\n</script>" _language="" /%}
